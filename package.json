{
  "name": "udps",
  "version": "0.1.0",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "build-dev": "better-npm-run build-dev",
    "test": "mocha --compilers js:babel-register",
    "lint": "eslint --fix ."
  },
  "betterScripts": {
    "start": {
      "command": "node server.js",
      "env": {
        "DEBUG": true
      }
    },
    "build-dev": {
      "command": "webpack -p",
      "env": {
        "DEBUG": true
      }
    },
    "build": {
      "command": "webpack -p",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
